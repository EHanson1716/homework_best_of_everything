<div class="page-header">
  <h1>Neighborhood #<%= @neighborhood.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>Name</dt>
  <dd><%= @neighborhood.name %></dd>

  <dt>City</dt>
  <dd><%= @neighborhood.city %></dd>

  <% venues= Venue.where({ :neighborhood_id => @neighborhood.id }) %>

  <dt>Venues in this area</dt>
  <% venues.each do |venue| %>
  <dd>
    <%= Venue.find_by({ :id => venue.id }).name %>
  </dd>
  <% end %>


</dl>
